
public class Test12 {

	public static void main(String[] args) {

		int a; // a변수 선언
		int b; // b변수 선언
		int c; // c변수 선언
		int d; // d변수 선언

		a = 10 + 2; // a 수식
		b = 10 - 2; // b 수식
		c = 10 * 2; // c 수식
		d = 10 % 2; // d 수식

		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);

	}

}
