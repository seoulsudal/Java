
public class Student extends Person {

	// 필드 생성
	private final int FIRST_GRADER = 1;		// 1학년
	private final int SECOND_GRADER = 1;	// 2학년
	private final int THIRD_GRADER = 1;		// 3학년
	private final int FOURTH_GRADER = 1;	// 4학년

	// 메소드 생성
	// 기본 생성자
	public Student() {
		super();
	}

	// 중복 생성자
	public Student(String name, String address, String number, String email) {
		super(name, address, number, email);
	}

	// 1학년 반환
	public int getFIRST_GRADER() {
		return FIRST_GRADER;
	}

	// 2학년 반환
	public int getSECOND_GRADER() {
		return SECOND_GRADER;
	}

	// 3학년 반환
	public int getTHIRD_GRADER() {
		return THIRD_GRADER;
	}

	// 4학년 반환
	public int getFOURTH_GRADER() {
		return FOURTH_GRADER;
	}

	// 출력
	@Override
	public String toString() {
		return "Student [getYear()=" + getYear() + ", getFIRST_GRADER()=" + getFIRST_GRADER() + ", getSECOND_GRADER()="
				+ getSECOND_GRADER() + ", getTHIRD_GRADER()=" + getTHIRD_GRADER() + ", getFOURTH_GRADER()="
				+ getFOURTH_GRADER() + ", getName()=" + getName() + ", getAddress()=" + getAddress() + ", getNumber()="
				+ getNumber() + ", getEmail()=" + getEmail() + "]";
	}

}
