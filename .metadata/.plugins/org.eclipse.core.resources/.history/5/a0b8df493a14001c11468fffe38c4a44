

import Bag.*;

public class BagTest {

	public static void main(String[] args) {

		Bag<Book, String> bag = new Bag<Book, String>(new Book(), "과학");
		Bag<PencilCase, String> bag2 = new Bag<PencilCase, String>(new PencilCase(), "다이소");
		Bag<Notebook, String> bag3 = new Bag<Notebook, String>(new Notebook(), "삼줄노트");
		
		bag.showType();
		bag2.showType();
		bag3.showType();
		
		System.out.println("Thing is : " + bag.getThing());
		System.out.println("Name is : " + bag.getName());
		System.out.println("Thing is : " + bag2.getThing());
		System.out.println("Name is : " + bag2.getName());
		System.out.println("Thing is : " + bag3.getThing());
		System.out.println("Name is : " + bag3.getName());
	}

}
