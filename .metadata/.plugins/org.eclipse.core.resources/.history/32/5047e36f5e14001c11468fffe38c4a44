package linkedlist;

import java.util.LinkedList;

import vector.Print;

public class Test05 {

	public static void main(String[] args) {

		LinkedList<String> list = new LinkedList<String>();
		Print p = new Print();
		
		list.add("서울");
		list.add("북경");
		list.add("상해");
		
		for(int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i));
		}
		
		list.add(1, "LA");
		p.Print(1, list);
		list.addFirst("런던");
		p.Print(2, list);
		list.addLast("서울");
		p.Print(3, list);
		
		list.offer("파리");
		p.Print(4, list);
		list.offerFirst("로마");
		p.Print(5, list);
		list.offerLast("베른");
		p.Print(6, list);
		
		System.out.println("7 : " + list.peek());
		System.out.println("8 : " + list.peekFirst());
		System.out.println("9 : " + list.peekLast());
	}

}
