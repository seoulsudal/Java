import java.util.Scanner;

public class CalculateTheVolumeOfTheCylinder {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		
		double radius;		//반지름 변수 서언
		double length;		//길이 변수 선언
		double area;		//넓이 변수 선언
		double volme;		//부피 변수 선언
		double pi;			//원주율 변수 선언
		
		pi = 3.14;			//원주율 값 입력
		
		System.out.println("반지름을 입력하세요.");
		radius = input.nextDouble();	//반지름 입력
		
		System.out.println("길이를 입력하세요.");
		length = input.nextDouble();	//길이 입력
		
		area = radius * radius * pi;	//넓이 공식 입력
		volme = area * length;			//부피 공식 입력
		
		System.out.print("반지름이 "+radius+"이고, 길이가 "+length+"인 원의 부피는 ");
		System.out.printf("%.2f 이다",volme);
		

	}

}
