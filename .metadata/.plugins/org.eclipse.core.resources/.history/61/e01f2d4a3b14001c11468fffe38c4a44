package Bag;

public class Bag<T extends Solid> {

	private T thing;

	public Bag(T thing) {
		this.thing = thing;
	}

	public T getThing() {
		return thing;
	}

	public void setThing(T thing) {
		this.thing = thing;
	}

	public void showType() { 
		System.out.println("T의 타입은 " +	thing.getClass().getName());
	}

}
