
public class Greeting {

	public static void main(String[] args) {
		// 첫번째 방법
		// String name;
		// name = StringPrint("홍길동");
		// System.out.println(name);
		// 두번째 방법
		StringPrint("홍길동");
	}
	
	// 하나의 문자열 출력 메소드
	public static void StringPrint(String str) {
		// 첫번째 방법
		// String hi;
		// hi = " 안녕!";
		// str + hi;
		// 두번째 방법
		System.out.println(str+" 안녕!");
		}
	
}
